{
  "expo": {
    "name": "f0-react-native",
    "slug": "f0-react-native",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "f0reactnative",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "icon": {
        "dark": "./assets/images/ios-dark.png",
        "light": "./assets/images/ios-light.png",
      },
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#E6F4FE"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      [
        "expo-router",
        {
          "root": "./playground/app"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/factorial.ttf",
            "./assets/fonts/Inter/InterNormal.ttf",
            "./assets/fonts/Inter/InterMedium.ttf",
            "./assets/fonts/Inter/InterSemiBold.ttf",
            "./assets/fonts/Inter/InterBold.ttf",
            "./assets/fonts/OxaniumMedium.ttf"
          ]
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ECEDEE",
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "354f5148-fa6f-45dd-8869-991a24b786e0"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/354f5148-fa6f-45dd-8869-991a24b786e0"
    }
  }
}
